
public class Homework6 {
	public static void main(String[] args) {
		int[] a = {222,33,44,5,55}; 
		
		for(int i = 0; i < a.length; i++) {
			int temp = i;
			for(int j = i; j < a.length; j++) {
				if(a[j] < a[temp]) {
					temp = j;
				}	
			}
			int tmp = a[temp];
			a[temp] = a[i];
			a[i] = tmp;
		}
		for(int i = 0; i < a.length; i++) {
			System.out.print(a[i] + " ");			
		}
	}
}
