
public class Homework5 {
	public static void main(String[] args) {
		int[] a = {9,8,7,6,5,4,3,2,1};
		for(int i = 0; i < a.length - 1; i++) {
			for(int j =0; j < a.length -1 - i; j++) {
				if(a[j] > a[j+1]) {
					int temp = a[j+1];
					a[j+1] = a[j];
					a[j] = temp;
				}
			}
		}
		for(int i =0;i < a.length; i++) {
			System.out.print(a[i] + " ");
		}
		
	}
}
